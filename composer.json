{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "awcodes/filament-gravatar": "^2.0",
        "aymanalhattami/filament-slim-scrollbar": "^2.0",
        "bezhansalleh/filament-language-switch": "^3.1",
        "bezhansalleh/filament-shield": "^3.2",
        "filament/filament": "^3.2",
        "hasnayeen/themes": "^3.0",
        "icetalker/filament-stepper": "^1.2",
        "icetalker/filament-table-repeater": "^1.2",
        "illuminate/contracts": "^11.3",
        "jeffgreco13/filament-breezy": "^2.3",
        "joshembling/image-optimizer": "^1.4",
        "laravel/framework": "^11.0",
        "laravel/tinker": "^2.9",
        "pxlrbt/filament-activity-log": "^1.1",
        "pxlrbt/filament-spotlight": "^1.2",
        "shuvroroy/filament-spatie-laravel-backup": "^2.1",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-package-tools": "^1.16",
        "swisnl/filament-backgrounds": "^1.1",
        "z3d0x/filament-logger": "^0.7.1"
    },
    "require-dev": {
        "dipeshsukhia/laravel-country-state-city-data": "^2.0",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
